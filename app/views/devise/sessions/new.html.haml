.lead Sign In
= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
  .form-group= f.email_field :email, autofocus: true, placeholder: "Email address", class: "form-control"
  .form-group= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "Password"
  = f.submit "Sign in", class: "btn btn-default"
